{
  "name": "spa-vite-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --mode sandbox --host",
    "dev:development": "vite --open --mode development --host",
    "dev:staging": "vite --open --mode staging",
    "build": "export NODE_ENV=sandbox && rm -rf /dist && vue-tsc && vite build",
    "preview": "export NODE_ENV=sandbox && rm -rf /dist && vite preview",
    "build:development": "export NODE_ENV=development && rm -rf /dist && vue-tsc && vite build",
    "build:staging": "export NODE_ENV=staging &&  rm -rf /dist && vue-tsc && vite build",
    "build:production": "export NODE_ENV=production &&  rm -rf /dist && vue-tsc && vite build",
    "lint:check": "eslint --ext .js,.vue,.ts ./",
    "lint": "eslint --ext .js,.vue,.ts --fix ./",
    "format:check": "prettier --list-different \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "format": "prettier --write \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "stylelint:check": "stylelint \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\"",
    "stylelint": "stylelint --fix \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\"",
    "fix:all": "yarn format && yarn lint && yarn stylelint",
    "check:all": "yarn format:check && yarn lint:check && yarn stylelint:check",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e-cy": "export NODE_ENV=local && cypress open",
    "test:e2e-cy-cli": "export NODE_ENV=local && cypress run --browser chrome",
    "test:dev:e2e-cy": "export NODE_ENV=development && cypress open",
    "test:dev:e2e-cy-cli": "export NODE_ENV=development && cypress run --browser chrome",
    "test:stg:e2e-cy": "export NODE_ENV=staging && cypress open",
    "test:stg:e2e-cy-cli": "export NODE_ENV=staging && cypress run --browser chrome",
    "test:prod:e2e-cy": "export NODE_ENV=production && cypress open",
    "test:prod:e2e-cy-cli": "export NODE_ENV=production && cypress run --browser chrome",
    "test": "vue-tsc --declaration --emitDeclarationOnly",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vueuse/head": "^1.1.15",
    "axios": "^1.3.4",
    "bulma": "^0.9.4",
    "dotenv": "^16.0.3",
    "pinia": "^2.0.32",
    "sass": "^1.58.3",
    "tslint": "^6.1.3",
    "vee-validate": "^4.7.4",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.14.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.9.0",
    "husky": ">=8",
    "lint-staged": ">=13",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.vue": [
      "prettier --list-different",
      "eslint --ext",
      "stylelint"
    ],
    "*.ts": [
      "prettier --list-different",
      "eslint --ext"
    ],
    "*.js": [
      "prettier --list-different",
      "eslint --ext"
    ],
    "*.scss": [
      "prettier --list-different",
      "stylelint"
    ],
    "*.css": [
      "prettier --list-different",
      "stylelint"
    ],
    "*.html": "prettier --list-different",
    "*.json": "prettier --list-different"
  }
}
