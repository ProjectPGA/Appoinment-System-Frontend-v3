{
  "name": "spa-vite-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --mode sandbox --host",
    "dev:development": "vite --open --mode development --host",
    "dev:staging": "vite --open --mode staging",
    "build": "export NODE_ENV=sandbox && rm -rf /dist && vue-tsc && vite build",
    "preview": "export NODE_ENV=sandbox && rm -rf /dist && vite preview",
    "build:development": "export NODE_ENV=development && rm -rf /dist && vue-tsc && vite build",
    "build:staging": "export NODE_ENV=staging &&  rm -rf /dist && vue-tsc && vite build",
    "build:production": "export NODE_ENV=production &&  rm -rf /dist && vue-tsc && vite build",
    "lint:check": "eslint --ext .js,.vue,.ts ./",
    "lint": "eslint --ext .js,.vue,.ts --fix ./",
    "format:check": "prettier --list-different \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "format": "prettier --write \"./**/*.?(vue|ts|js|scss|html|json)\"",
    "stylelint:check": "stylelint \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\"",
    "stylelint": "stylelint --fix \"{,!(node_modules|dist)/**/}*.{css,scss,vue}\"",
    "fix:all": "pnpm format && pnpm lint && pnpm stylelint",
    "check:all": "pnpm format:check && pnpm lint:check && pnpm stylelint:check",
    "test:unit": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e-cy-cli-action": "cypress run --browser chrome",
    "test:e2e-cy": "export NODE_ENV=local && cypress open",
    "test:e2e-cy-cli": "export NODE_ENV=local && cypress run --browser chrome",
    "test:dev:e2e-cy": "export NODE_ENV=development && cypress open",
    "test:dev:e2e-cy-cli": "export NODE_ENV=development && cypress run --browser chrome",
    "test:stg:e2e-cy": "export NODE_ENV=staging && cypress open",
    "test:stg:e2e-cy-cli": "export NODE_ENV=staging && cypress run --browser chrome",
    "test:prod:e2e-cy": "export NODE_ENV=production && cypress open",
    "test:prod:e2e-cy-cli": "export NODE_ENV=production && cypress run --browser chrome",
    "test": "vue-tsc --declaration --emitDeclarationOnly",
    "husky:prepare": "husky install && chmod ug+x .husky/*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.5.1",
    "axios-mock-adapter": "^1.22.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "sass": "^1.69.4",
    "stylelint-declaration-strict-value": "^1.9.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "vee-validate": "^4.11.8",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.3.7",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@faker-js/faker": "^8.2.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@pinia/testing": "^0.1.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/vue": "^7.0.0",
    "@types/jest": "^29.5.6",
    "@types/jsdom": "^21.1.4",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "cypress": "13.3.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.0",
    "happy-dom": "^12.9.1",
    "husky": ">=8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": ">=15",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.21"
  },
  "lint-staged": {
    "*.vue": [
      "prettier --list-different",
      "eslint --ext",
      "stylelint"
    ],
    "*.ts": [
      "prettier --list-different",
      "eslint --ext"
    ],
    "*.js": [
      "prettier --list-different",
      "eslint --ext"
    ],
    "*.scss": [
      "prettier --list-different",
      "stylelint"
    ],
    "*.css": [
      "prettier --list-different",
      "stylelint"
    ],
    "*.html": "prettier --list-different",
    "*.json": "prettier --list-different"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": "*",
    "node": ">=18"
  }
}
